{
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:5080"
      },
      "HttpsInlineCertFile": {
        "Url": "https://*:5081",
        "Certificate": {
          "Path": "/path/to/.pfx"
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
